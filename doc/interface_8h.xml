        <type>int</type>
        <definition>int jogar</definition>
        <argsstring>(ESTADO *state, COORDENADA c)</argsstring>
        <name>jogar</name>
        <param>
          <type><ref refid="data_8h_1a859d62f27ea4cc3e6db1a2bf2734d252" kindref="member">ESTADO</ref> *</type>
